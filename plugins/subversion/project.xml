<?xml version="1.0" encoding="UTF-8"?>
<project>
  <pomVersion>1</pomVersion>
  <artifactId>vulcan-subversion</artifactId>
  <groupId>vulcan</groupId>
  <id>vulcan-subversion</id>
  <name>vulcan-subversion</name>
  <currentVersion>0.9</currentVersion>
  <organization>
    <name>SourceForge</name>
    <url>http://vulcan.sourceforge.net/</url>
  </organization>
  <inceptionYear>2005</inceptionYear>
  <package>net.sourceforge.vulcan.subversion</package>
  <gumpRepositoryId>vulcan-subversion</gumpRepositoryId>
  <shortDescription>Subversion source repository implementation for Vulcan</shortDescription>
  <url>http://vulcan.sourceforge.net/plugins/subversion</url>
  <issueTrackingUrl>http://sourceforge.net/tracker</issueTrackingUrl>
  <siteAddress>http://vulcan.sourceforge.net/plugins/subversion</siteAddress>
  <siteDirectory>/home/groups/v/vu/vulcan/htdocs/plugins/subversion</siteDirectory>
  <distributionDirectory>/var/www/secure/projects/vulcan/nightly</distributionDirectory>
  <repository>
    <connection>scm:svn:ext:http://wush.net/svn/mg714/vulcan/plugins/subversion/trunk</connection>
    <url><![CDATA[http://wush.net/websvn/mg714/listing.php?repname=mg714&path=%2Fvulcan%2Fplugins%2Fsubversion%2Ftrunk%2F&rev=0&sc=0]]></url>
  </repository>
  <licenses>
    <license>
      <name>GNU General Public License, Version 2, June 1991</name>
      <url>http://www.gnu.org/licenses/gpl.txt</url>
      <distribution>repo</distribution>
      <comments>Only version 2 of the GPL may be used</comments>
    </license>
  </licenses>
  <developers>
    <developer>
      <name>Chris Eldredge</name>
      <id>chriseldredge</id>
      <email>chriseldredge@users.sourceforge.net</email>
      <timezone>-5</timezone>
    </developer>
  </developers>
  <dependencies>
    <dependency>
      <groupId>vulcan</groupId>
      <artifactId>vulcan</artifactId>
      <version>0.8</version>
      <type>jar</type>
      <url>http://vulcan.sourceforge.net</url>
    </dependency>
    <dependency>
      <groupId>org.tmate</groupId>
      <artifactId>svnkit</artifactId>
      <version>1.1.0</version>
      <type>jar</type>
      <url>http://svnkit.com</url>
      <properties><vulcan.bundle>true</vulcan.bundle></properties>
    </dependency>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.1</version>
      <type>jar</type>
      <url>http://www.junit.org/</url>
    </dependency>
    <dependency>
      <groupId>commons-jelly</groupId>
      <artifactId>commons-jelly</artifactId>
      <version>1.0-RC1</version>
      <type>jar</type>
      <url>http://jakarta.apache.org/commons/jelly</url>
    </dependency>
    <dependency>
      <groupId>commons-lang</groupId>
      <artifactId>commons-lang</artifactId>
      <version>2.0</version>
      <type>jar</type>
      <url>http://jakarta.apache.org/commons/lang</url>
    </dependency>
    <dependency>
      <groupId>commons-beanutils</groupId>
      <artifactId>commons-beanutils</artifactId>
      <version>1.7.0</version>
      <type>jar</type>
      <url>http://jakarta.apache.org/commons/beanutils</url>
    </dependency>
    <dependency>
      <groupId>commons-collections</groupId>
      <artifactId>commons-collections</artifactId>
      <version>3.1</version>
      <type>jar</type>
      <url>http://jakarta.apache.org/commons/collections</url>
    </dependency>
    <dependency>
      <groupId>commons-logging</groupId>
      <artifactId>commons-logging</artifactId>
      <version>1.0.4</version>
      <type>jar</type>
      <url>http://jakarta.apache.org/commons/logging</url>
    </dependency>
    <dependency>
      <groupId>easymock</groupId>
      <artifactId>easymock</artifactId>
      <version>1.1</version>
      <type>jar</type>
      <url>http://www.easymock.org/</url>
    </dependency>
    <dependency>
      <groupId>springframework</groupId>
      <artifactId>spring</artifactId>
      <version>1.2.4</version>
      <type>jar</type>
      <url>http://www.springframework.org</url>
    </dependency>
  </dependencies>
  <build>
    <sourceDirectory>source/main/java</sourceDirectory>
    <resources>
      <resource>
        <directory>source/main/java</directory>
        <includes>
          <include>**/*.properties</include>
          <include>**/*.xml</include>
        </includes>
      </resource>
    </resources>
    <unitTestSourceDirectory>source/test/java</unitTestSourceDirectory>
    <unitTest>
      <includes>
        <include>**/*Test.java</include>
      </includes>
    </unitTest>
    <jarResources>
      <includes>
        <include>**/*.xml</include>
        <include>**/*.properties</include>
      </includes>
    </jarResources>
  </build>
  <reports>
    <report>maven-junit-report-plugin</report>
	<report>maven-emma-plugin</report>
    <report>maven-jdepend-plugin</report>
    <report>maven-changes-plugin</report>
    <report>maven-changelog-plugin</report>
    <report>maven-file-activity-plugin</report>
    <report>maven-developer-activity-plugin</report>
    <report>maven-license-plugin</report>
    <report>maven-jxr-plugin</report>
  </reports>
</project>

